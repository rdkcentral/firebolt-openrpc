{
    "name": "JavaScript",
    "langcode": "javascript",
    "templatesPerModule": [
        "/index.mjs",
        "/defaults.mjs"
    ],
    "createModuleDirectories": true,
    "copySchemasIntoModules": true,
    "aggregateFile": "/index.d.ts",
    "treeshakePattern": "(import|export).*?from\\s+['\"](.*?)['\"]",
    "treeshakeTypes": [".js", ".mjs"],
    "operators": {
        "or": " | ",
        "stringQuotation": "'"
    },
    "primitives": {
        "boolean": "boolean",
        "integer": "number",
        "number": "number",
        "string": "string",
        "object": "object"
    }
}